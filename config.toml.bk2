data_dir = "./data"
auto_check = true
auto_water = true
auto_check_period = 0
auto_water_period = 0
auto_water_empty = false
auto_water_young = false
auto_water_ready = false
auto_water_old = false
auto_water_unknown = false
water_empty_duration = 0
water_young_duration = 0
water_ready_duration = 0
water_old_duration = 0
water_unknown_duration = 0
positions = [[150, 0], [200, 135], [260, 40], [360, 40]]

[actuators.en_pin]
chip = "/dev/gpiochipAGV"
line = 2

[actuators.linear_x]
reverse = false
limit = 360
position = 0
min_mm_per_s = 20
max_mm_per_s = 50
step_per_mm = 40

[actuators.linear_x.step_pin]
chip = "/dev/gpiochipAGV"
line = 3

[actuators.linear_x.dir_pin]
chip = "/dev/gpiochipAGV"
line = 4

[actuators.linear_y]
reverse = false
limit = 320
position = 0
min_mm_per_s = 20
max_mm_per_s = 50
step_per_mm = 40

[actuators.linear_y.step_pin]
chip = "/dev/gpiochipAGV"
line = 5

[actuators.linear_y.dir_pin]
chip = "/dev/gpiochipAGV"
line = 6

[actuators.watering.pin]
chip = "/dev/gpiochipAGV"
line = 7

[actuators.camera]
video_path = "/dev/video3"
video_orientation = "Normal"

[detector]
model_path = "./best.safetensors"
model_size = "S"
num_classes = 4
nms_threshold = 0.5
acc_threshold = 0.20000000298023224
